{
  "name": "web-ssh-tool",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build:web": "cd web && npm run build",
    "build:node": "cd node && npm run build",
    "build:bundle": "npx ts-node build.ts",
    "build": "npm run build:web && npm run build:node && npm run build:bundle",
    "clean:web": "cd web && npm ci",
    "clean:node": "cd node && npm ci",
    "clean": "npm run clean:web && npm run clean:node && npm ci",
    "rebuild": "npm run clean && npm run build",
    "deploy": "npx ts-node deploy.ts"
  },
  "author": "JohnGu9",
  "license": "MIT",
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "multer": "^1.4.3",
    "ssh2": "^1.5.0",
    "tsee": "^1.3.2",
    "uuid": "^8.3.2",
    "ws": "^8.2.3"
  },
  "devDependencies": {
    "@types/license-checker": "^25.0.3",
    "@types/node": "^16.11.9",
    "@types/uuid": "^8.3.3",
    "license-checker": "^25.0.1",
    "pkg": "^5.4.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.5",
    "utf-8-validate": "^5.0.7"
  },
  "main": "index.js",
  "bin": "index.js",
  "pkg": {
    "scripts": "index.js",
    "assets": [
      "web/build/*",
      "web/build/static/**/*",
      "node/https/*"
    ],
    "targets": [
      "node16-linux-x64",
      "node16-macos-x64"
    ],
    "outputPath": "build"
  },
  "engines": {
    "node": ">=12.13"
  }
}